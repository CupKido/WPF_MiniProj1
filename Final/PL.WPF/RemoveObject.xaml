<Window x:Class="PL.WPF.RemoveObject"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:PL.WPF"
        mc:Ignorable="d"
        Title="RemoveObject" Height="200" Width="500">
    <Grid>
        <TextBlock x:Name="KeyReq" HorizontalAlignment="Left" Margin="29,25,0,0" TextWrapping="Wrap" Text="Enter Key:" FontSize="16" VerticalAlignment="Top" Height="29" Width="100"/>
        <TextBox x:Name="KeyData" HorizontalAlignment="Left" Height="29" Margin="134,25,0,0" TextWrapping="Wrap" Text="" FontSize="16" VerticalAlignment="Top" Width="120"/>
        <Button x:Name="RemoveButton" Content="Remove" Click="RemoveObj" HorizontalAlignment="Left" Margin="327,111,0,0" VerticalAlignment="Top" Width="120" Height="30"/>

    </Grid>
</Window>
