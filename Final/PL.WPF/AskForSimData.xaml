<Window x:Class="PL.WPF.AskForSimData"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:PL.WPF"
        mc:Ignorable="d"
        Title="AskForData" Height="200" Width="400">
    <Grid>
        <TextBlock x:Name="AskTimeTBL" HorizontalAlignment="Left" Margin="50,38,0,0" TextWrapping="Wrap" Text="Please Insert Time:" VerticalAlignment="Top" Height="21" Width="123"/>
        <TextBox x:Name="AskDataTBO" HorizontalAlignment="Left" Height="21" Margin="189,37,0,0" TextWrapping="Wrap" Text="00:00:00" VerticalAlignment="Top" Width="163"/>
        <TextBlock x:Name="AskSecondTBL" HorizontalAlignment="Left" Margin="10,64,0,0" TextWrapping="Wrap" Text="Please Insert Second (MS):" VerticalAlignment="Top" Height="21" Width="147"/>
        <TextBox x:Name="AskSecondTBO" HorizontalAlignment="Left" Height="21" Margin="189,64,0,0" TextWrapping="Wrap" Text="" VerticalAlignment="Top" Width="163"/>
        <Button x:Name="AskDataButton" Content="Insert" Click="AskDataButton_Click" HorizontalAlignment="Left" Margin="261,130,0,0" VerticalAlignment="Top" Width="117" Height="30"/>
        

    </Grid>
</Window>
